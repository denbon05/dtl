{
  "name": "dtl",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "npx vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "npx vite preview",
    "test:unit": "npx vitest",
    "test:e2e": "npx playwright test",
    "test": "npm run test:unit && npm run test:e2e",
    "build-only": "npx vite build",
    "type-check": "npx vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "npx eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore",
    "format": "npx prettier --write src/",
    "postinstall": "npx playwright install --with-deps"
  },
  "license": "MIT",
  "repository": {
    "url": "https://github.com/denbon05/dtl.git"
  },
  "dependencies": {
    "vue": "^3.3.4",
    "vue-router": "^4.2.5",
    "vuetify": "^3.3.23"
  },
  "devDependencies": {
    "@mdi/font": "^7.3.67",
    "@playwright/test": "^1.39.0",
    "@rushstack/eslint-patch": "^1.3.3",
    "@tsconfig/node18": "^18.2.2",
    "@types/jsdom": "^21.1.3",
    "@types/node": "^18.18.5",
    "@vitejs/plugin-vue": "^4.4.0",
    "@vitejs/plugin-vue-jsx": "^3.0.2",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.1",
    "@vue/tsconfig": "^0.4.0",
    "eslint": "^8.49.0",
    "eslint-plugin-vue": "^9.17.0",
    "jsdom": "^22.1.0",
    "npm-run-all2": "^6.1.1",
    "prettier": "^3.0.3",
    "typescript": "~5.2.0",
    "vite": "^4.4.11",
    "vitest": "^0.34.6",
    "vue-tsc": "^1.8.19"
  }
}